    "@fontsource/inter": "^4.5.14",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/styles": "^5.11.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "axios": "^1.2.1",
    "cheerio": "^1.0.0-rc.12",
    "electron-chromedriver": "^22.0.0",
    "electron-is-dev": "^2.0.0",
    "moment": "^2.29.4",
    "puppeteer-core": "^19.4.1",
    "puppeteer-in-electron": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-images-uploading": "^3.1.7",
    "react-live-clock": "^6.1.1",
    "react-material-ui-carousel": "^2.3.10-mui5",
    "react-moment": "^1.1.2",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.6",
    "sass": "^1.57.1",
    "selenium-webdriver": "^4.7.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "concurrently \"yarn react-scripts start\" \"yarn electron\" ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "wait-on http://localhost:3000 && electron .",
    "electron:build": "rm -rf ./dist && rm -rf ./build && yarn build && electron-builder -c.extraMetadata.main=build/main.js"
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron-cra",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "wait-on": "^7.0.1"
  }
}
